<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aduMapper">
  
    <update id="updateadu" parameterType="com.waffle.vo.AduVO">
		UPDATE adu
		   SET temp  =   #{temp,jdbcType=VARCHAR}
		   , 	wet  =   #{wet,jdbcType=VARCHAR}
	</update>
</mapper>